{
  "version": 1,
  "hooks": {
    "sessionStart": [
      {
        "command": "~/.cursor/hooks/start-telemetry.sh"
      }
    ],
    "sessionEnd": [
      {
        "command": "~/.cursor/hooks/end-telemetry.sh"
      }
    ],
    "preToolUse": [
      {
        "command": "~/.cursor/hooks/log-tool.sh"
      }
    ],
    "postToolUse": [
      {
        "command": "~/.cursor/hooks/log-tool.sh"
      }
    ],
    "postToolUseFailure": [
      {
        "command": "~/.cursor/hooks/log-tool-failure.sh"
      }
    ],
    "beforeSubmitPrompt": [
      {
        "command": "~/.cursor/hooks/log-prompt.sh"
      }
    ]
  }
}
